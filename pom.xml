        <sharding-jdbc.version>4.1.1</sharding-jdbc.version>
        <spring-dubbo.version>3.1.0</spring-dubbo.version>
        <zkclient.version>0.11</zkclient.version>
        <seata-spring.version>1.4.2</seata-spring.version>
        <spring-mybatis.version>2.2.2</spring-mybatis.version>
        <apache.curator.version>5.3.0</apache.curator.version>
        <commons-pool2.version>2.11.1</commons-pool2.version>
        <commons-lang3.version>3.12.0</commons-lang3.version>
        <commons-codec.version>1.15</commons-codec.version>
        <gson.version>2.9.1</gson.version>
        <java-jwt.version>4.0.0</java-jwt.version>
        <redisson.version>3.17.6</redisson.version>
        <elasticsearch.version>8.4.1</elasticsearch.version>
        <zxing.version>3.5.0</zxing.version>
